/* Trying to produce min, max and average values. */

#include "../../std_lib_facilities.h"

int main()
{
	double temp = 0.0;
	double sum = 0.0;
	double high_temp = -1000;
	double low_temp = 1000;
	int no_of_temps = 0;

	while (cin >> temp) {
		++no_of_temps;
		sum += temp;
		if (temp > high) high_temp = temp;
		if (temp < low) low_temp = temp;
	}

	cout << "High temperature:" << high_temp << endl;
	cout << "Low temperature:" << high_temp << endl;
	cout << "Average temperature:" << sum / no_of_temps << endl;
}

